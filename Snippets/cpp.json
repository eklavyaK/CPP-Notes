			"#define S second",
			"#define int long long",
			"typedef long double ld;",
			"using namespace std;",
			"",
			"",
			"",
			"void code(){",
			"\t$0",
			"",
			"}",
			"",
			"signed main(){",
    		"\tcin.tie(0)->sync_with_stdio(0);",
    		"\tint t; cin>>t; while(t--)code();",
    		"\treturn 0;",
			"}",
		],
		"description": "Log output to console"
	}
}